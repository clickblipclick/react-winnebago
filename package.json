{
  "name": "react-winnebago",
  "version": "0.1.1",
  "description": "A set of form validation components for React.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "precommit": "lint-staged",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --write",
      "git add"
    ],
    "demo/src/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.5.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "form-serialize": "^0.7.2",
    "husky": "^0.14.3",
    "immutability-helper": "^2.3.1",
    "lint-staged": "^4.0.3",
    "normalize.css": "^7.0.0",
    "nwb": "0.18.x",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "author": "Ben Nyberg",
  "homepage": "https://heyben.com",
  "license": "MIT",
  "repository": "https://github.com/clickblipclick/react-winnebago.git",
  "keywords": [
    "react-component"
  ]
}
